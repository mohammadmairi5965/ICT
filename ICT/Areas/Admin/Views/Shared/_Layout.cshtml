<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title - مدیریت </title>
    <meta name="description" content="@ViewBag.Description" />
    <meta name="keywords" content="@ViewBag.keyword" />
    <meta keywords="@ViewBag.keyword1" />

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Core CSS - Include with every page -->
    <link href="/Areas/Admin/Content/css/bootstrap-rtl.css" rel="stylesheet">
    <link href="/Areas/Admin/Content/font-awesome/css/font-awesome.css" rel="stylesheet">

    <!-- Page-Level Plugin CSS - Blank -->
    <!-- SB Admin CSS - Include with every page -->
    <link href="/Areas/Admin/Content/css/sb-admin.css" rel="stylesheet">
    <link href="/Areas/Admin/Content/css/fstdropdown.min.css" rel="stylesheet" />
    <script src="/Areas/Admin/Content/css/fstdropdown.min.js"></script>
    <link href="~/Areas/Admin/Content/Calculate/js-persian-cal.css" rel="stylesheet" />
    <link href="~/Content/sw/sweetalert2.min.css" rel="stylesheet" />
   

</head>

<body>

    <div id="wrapper">

        <!--111-->
        @Html.Action("Header", "Home")


        <!--  1111-->

        <div id="page-wrapper">


            @RenderBody()
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
    <!-- Core Scripts - Include with every page -->

    <script src="/Areas/Admin/Content/js/jquery-1.10.2.js"></script>
    <script src="/Areas/Admin/Content/js/bootstrap.min.js"></script>
    <script src="/Areas/Admin/Content/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="/Areas/Admin/Content/js/sb-admin.js"></script>
    <script src="/Scripts/jquery.validate.min.js"></script>
    <script src="/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Areas/Admin/Content/Calculate/js-persian-cal.min.js"></script>
    <script src="~/Content/sw/sweetalert2.min.js"></script>
    @*<script src="~/Content/sw/sweetalert2.all.min.js"></script>*@
    @RenderSection("scripts", required: false)
</body>

</html>
